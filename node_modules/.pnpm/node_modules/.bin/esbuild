#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Git/assetlists/node_modules/.pnpm/esbuild-wasm@0.19.12/node_modules/esbuild-wasm/bin/node_modules:/mnt/e/Git/assetlists/node_modules/.pnpm/esbuild-wasm@0.19.12/node_modules/esbuild-wasm/node_modules:/mnt/e/Git/assetlists/node_modules/.pnpm/esbuild-wasm@0.19.12/node_modules:/mnt/e/Git/assetlists/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/Git/assetlists/node_modules/.pnpm/esbuild-wasm@0.19.12/node_modules/esbuild-wasm/bin/node_modules:/mnt/e/Git/assetlists/node_modules/.pnpm/esbuild-wasm@0.19.12/node_modules/esbuild-wasm/node_modules:/mnt/e/Git/assetlists/node_modules/.pnpm/esbuild-wasm@0.19.12/node_modules:/mnt/e/Git/assetlists/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esbuild-wasm/bin/esbuild" "$@"
else
  exec node  "$basedir/../esbuild-wasm/bin/esbuild" "$@"
fi
