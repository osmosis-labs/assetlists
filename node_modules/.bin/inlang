#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Git/assetlists/node_modules/.pnpm/@inlang+cli@1.20.0_patch_hash=3rvxmsc6ms375osi6awiqx72gq/node_modules/@inlang/cli/bin/node_modules:/mnt/e/Git/assetlists/node_modules/.pnpm/@inlang+cli@1.20.0_patch_hash=3rvxmsc6ms375osi6awiqx72gq/node_modules/@inlang/cli/node_modules:/mnt/e/Git/assetlists/node_modules/.pnpm/@inlang+cli@1.20.0_patch_hash=3rvxmsc6ms375osi6awiqx72gq/node_modules/@inlang/node_modules:/mnt/e/Git/assetlists/node_modules/.pnpm/@inlang+cli@1.20.0_patch_hash=3rvxmsc6ms375osi6awiqx72gq/node_modules:/mnt/e/Git/assetlists/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/Git/assetlists/node_modules/.pnpm/@inlang+cli@1.20.0_patch_hash=3rvxmsc6ms375osi6awiqx72gq/node_modules/@inlang/cli/bin/node_modules:/mnt/e/Git/assetlists/node_modules/.pnpm/@inlang+cli@1.20.0_patch_hash=3rvxmsc6ms375osi6awiqx72gq/node_modules/@inlang/cli/node_modules:/mnt/e/Git/assetlists/node_modules/.pnpm/@inlang+cli@1.20.0_patch_hash=3rvxmsc6ms375osi6awiqx72gq/node_modules/@inlang/node_modules:/mnt/e/Git/assetlists/node_modules/.pnpm/@inlang+cli@1.20.0_patch_hash=3rvxmsc6ms375osi6awiqx72gq/node_modules:/mnt/e/Git/assetlists/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@inlang/cli/bin/run.js" "$@"
else
  exec node  "$basedir/../@inlang/cli/bin/run.js" "$@"
fi
